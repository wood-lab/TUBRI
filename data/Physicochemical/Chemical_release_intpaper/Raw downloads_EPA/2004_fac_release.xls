               + NVL(R_TE.UNINJ_IIV,0) + NVL(R_TE.LAND_TREA,0) + NVL(R_TE.SI_5_5_3A,0) + NVL(R_TE.SI_5_5_3B,0) + NVL(R_TE.OTH_DISP,0)  + NVL(R_TE.M81,0)
               + NVL(R_TE.M65,0) + NVL(R_TE.M64,0) + NVL(R_TE.M10,0)  + NVL(R_TE.M41,0) + NVL(R_TE.M62,0) + NVL(R_TE.M82,0) + NVL(R_TE.M66,0)
               + NVL(R_TE.M67,0) + NVL(R_TE.M73,0) + NVL(R_TE.M79,0) + NVL(R_TE.M90,0) + NVL(R_TE.M91,0) + NVL(R_TE.M94,0) + NVL(R_TE.M99,0) + NVL(R_TE.M71,0),7) END TOTAL_ONOFFSITE
from TRI_EX_RELEASE_TOX_REPORT_INFO TE
inner join TRI_EX_RELEASE_REPORT_INFO R_TE
on TE.DOC_CTRL_NUM = R_TE.DOC_CTRL_NUM
and R_TE.TRI_CHEM_ID = 'N150'
left outer join (select TF.TRI_FACILITY_ID, count(distinct doc_ctrl_num) NUM_R_FORMS
from TRI_EX_RELEASE_TOX_REPORT_INFO TF
where TF.FORM_TYPE_IND = 'L'
and TF.TRI_CHEM_ID = 'N150' and  REPORTING_YEAR = '2004' and TF.ZIP_CODE = '70427' group by TF.TRI_FACILITY_ID) R_FORM
      on TE.TRI_FACILITY_ID = R_FORM.TRI_FACILITY_ID
      left outer join (select TF.TRI_FACILITY_ID, count(distinct doc_ctrl_num) NUM_A_FORMS

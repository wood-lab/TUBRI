<!-- DIOXIN query:  Select * from (  select FACILITY_NAME , FACILITY_ADDRESS,
      TE.TRI_FACILITY_ID FACILITY_ID,
      nvl(R_FORM.NUM_R_FORMS,0) NUM_R, nvl(A_FORM.NUM_A_FORMS,0) NUM_A,  
      TRI_CENTROID_LAT AS tri_centroid_lat,
      TRI_CENTROID_LONG AS tri_centroid_long,
      SUM(TE.UNINJ_I) AS UNINJ_I,
    SUM(TE.RCRA_C) AS RCRA_C,
    CASE WHEN SUM(TE.OTH_LANDF) IS NULL and SUM(TE.LANDF8795) IS NULL THEN NULL
        ELSE SUM(NVL(TE.OTH_LANDF,0))+ SUM(NVL(TE.LANDF8795,0)) END AS OTH_LANDF,
    CASE WHEN SUM(TE.UNINJ_I) IS NULL and SUM(TE.RCRA_C) IS NULL AND SUM(TE.OTH_LANDF) IS NULL AND SUM(TE.LANDF8795) IS NULL THEN NULL
        ELSE SUM(NVL(TE.UNINJ_I,0)) + SUM(NVL(TE.RCRA_C,0))
           + SUM(NVL(TE.OTH_LANDF,0)) + SUM(NVL(TE.LANDF8795,0)) END AS SUBTOT_LAND_ONSITE,
    SUM(TE.AIR_FUG) AS AIR_FUG,
    SUM(TE.AIR_STACK) AS AIR_STACK,
    SUM(TE.TOTAL_AIR_RELEASE) AS AIRLBY,
    SUM(TE.WATER) AS WATER,
        CASE WHEN SUM(TE.UNINJ8795) IS NULL and SUM(TE.UNINJ_IIV) IS NULL THEN NULL
        ELSE SUM(NVL(TE.UNINJ8795,0))+ SUM(NVL(TE.UNINJ_IIV,0)) END AS UNINJ_IIV,
        CASE WHEN SUM(TE.UNINJ8795) IS NULL and SUM(TE.UNINJ_I) IS NULL and SUM(TE.UNINJ_IIV) IS NULL THEN NULL
        ELSE SUM(NVL(TE.UNINJ8795,0)) + SUM(NVL(TE.UNINJ_I,0))
           + SUM(NVL(TE.UNINJ_IIV,0)) END AS E4,
        SUM(TE.OTH_LANDF) AS E51B,
        SUM(TE.LAND_TREA) AS E52,
    CASE WHEN SUM(SI_5_5_3A) IS NULL AND SUM(SI_5_5_3B) IS NULL AND SUM(SURF_IMP) IS NULL THEN NULL
        ELSE SUM(NVL(TE.SI_5_5_3A,0)) + SUM(NVL(TE.SI_5_5_3B,0)) + SUM(NVL(TE.SURF_IMP,0)) END AS E53,
        SUM(TE.OTH_DISP) AS E54,
    CASE WHEN SUM(LANDF8795) IS NULL and SUM(OTH_LANDF) IS NULL  and SUM(RCRA_C) IS NULL THEN NULL
        ELSE SUM(NVL(LANDF8795,0)) + SUM(NVL(OTH_LANDF,0)) + SUM(NVL(RCRA_C,0)) END AS E51,
    CASE WHEN SUM(TE.RCRA_C) IS NULL and SUM(TE.OTH_LANDF) IS NULL AND SUM(TE.LAND_TREA) IS NULL
        AND SUM(TE.LANDF8795) IS NULL AND SUM(SI_5_5_3B) IS NULL AND SUM(TE.OTH_DISP) IS NULL AND SUM(SURF_IMP) IS NULL THEN NULL
        ELSE SUM(NVL(TE.RCRA_C,0)) + SUM(NVL(TE.OTH_LANDF,0))
           + SUM(NVL(TE.LAND_TREA,0))
           + SUM(NVL(TE.LANDF8795,0)) + SUM(NVL(TE.OTH_DISP,0))
           + SUM(NVL(TE.SI_5_5_3B,0)) + SUM(NVL(TE.SURF_IMP,0)) END AS E5,
        SUM(TE.LAND_TREA) AS LAND_TREA,
    SUM(TE.SI_5_5_3A) AS SI_5_5_3A,
    CASE WHEN SUM(TE.SI_5_5_3B) IS NULL and SUM(TE.SURF_IMP) IS NULL THEN NULL
        ELSE SUM(NVL(TE.SI_5_5_3B,0))+ SUM(NVL(TE.SURF_IMP,0)) END AS SI_5_5_3B,
    SUM(TE.OTH_DISP) AS OTH_DISP,
    SUM(TE.SUBTOT_OTHER_ONSITE) AS SUBTOT_OTHER_ONSITE,
    SUM(TE.TOTAL_ONSITE_RELEASE) AS TOTAL_ONSITE,
     CASE WHEN '2004' >= '1996' THEN
        CASE WHEN SUM(TE.M81) IS NULL and SUM(TE.M71) IS NULL THEN NULL
        ELSE SUM(NVL(TE.M81,0)) + SUM(NVL(TE.M71,0)) END
     ELSE
        CASE WHEN SUM(TE.M81) IS NULL THEN NULL
        ELSE SUM(TE.M81) END END AS M81,
    SUM(TE.M65) AS M65,
     CASE WHEN '2004' >= '1996' THEN
        CASE WHEN SUM(TE.M64) IS NULL and SUM(TE.M72) IS NULL THEN NULL
        ELSE SUM(NVL(TE.M64,0)) + SUM(NVL(TE.M72,0)) END
     ELSE
        CASE WHEN SUM(TE.M64) IS NULL THEN NULL
        ELSE SUM(TE.M64) END END AS M64,
    CASE WHEN '2004' = '2002' THEN
            CASE WHEN SUM(TE.M81) IS NULL and SUM(TE.M65) IS NULL AND SUM(TE.M64) IS NULL AND SUM(TE.M71) IS NULL
                 AND SUM(TE.P81C) IS NULL AND SUM(TE.M82) IS NULL AND SUM(TE.M72) IS NULL THEN NULL
            ELSE SUM(NVL(TE.M81,0)) + SUM(NVL(TE.M65,0))
               + SUM(NVL(TE.M64,0)) + SUM(NVL(TE.M71,0))
               + SUM(NVL(TE.M82,0)) + SUM(NVL(TE.M72,0)) END
          WHEN '2004' >= '2002' THEN
            CASE WHEN SUM(TE.M81) IS NULL and SUM(TE.M65) IS NULL AND SUM(TE.M64) IS NULL AND SUM(TE.M72) IS NULL THEN NULL
            ELSE SUM(NVL(TE.M81,0)) + SUM(NVL(TE.M65,0)) + SUM(NVL(TE.P81C,0))
               + SUM(NVL(TE.M64,0)) + SUM(NVL(TE.M72,0)) END
          ELSE
            CASE WHEN SUM(TE.M81) IS NULL and SUM(TE.M65) IS NULL AND SUM(TE.M64) IS NULL AND SUM(TE.P81C) IS NULL THEN NULL
            ELSE SUM(NVL(TE.M81,0)) + SUM(NVL(TE.M65,0)) + SUM(NVL(TE.P81C,0))
               + SUM(NVL(TE.M64,0)) END END AS SUBTOT_LAND_OFFSITE,
    SUM(TE.M10) AS M10,
        CASE WHEN SUM(TE.M40M) IS NULL and SUM(TE.M41) IS NULL THEN NULL
        ELSE SUM(NVL(TE.M40M,0)) + SUM(NVL(TE.M41,0)) END AS M41,
        CASE WHEN SUM(TE.M61M) IS NULL and SUM(TE.M62) IS NULL THEN NULL
        ELSE SUM(NVL(TE.M61M,0)) + SUM(NVL(TE.M62,0)) END AS M62,
    SUM(TE.P81D) AS P81D,
    SUM(TE.P81C) AS P81C,
    CASE WHEN '2004' >= '2003' THEN
      CASE WHEN SUM(TE.M82) IS NULL AND SUM(TE.M71) IS NULL THEN NULL
      ELSE SUM(NVL(TE.M82,0)) + SUM(NVL(TE.M71,0)) END
    ELSE
      CASE WHEN SUM(TE.M82) IS NULL THEN NULL
      ELSE SUM(NVL(TE.M82,0)) END END AS M82,
    SUM(TE.M66) AS M66,
    CASE WHEN '2004' >= '2003' THEN
      CASE WHEN SUM(TE.M67) IS NULL AND SUM(TE.M63) IS NULL THEN NULL
      ELSE SUM(NVL(TE.M67,0)) + SUM(NVL(TE.M63,0)) END
    ELSE
      CASE WHEN SUM(TE.M67) IS NULL THEN NULL
      ELSE SUM(NVL(TE.M67,0)) END END AS M67,
    SUM(TE.M73) AS M73,
    SUM(TE.M79) AS M79,
    SUM(TE.M90) AS M90,
    SUM(TE.M91) AS M91,
  CASE WHEN SUM(TE.M94) IS NULL and SUM(TE.M91) IS NULL THEN NULL
        ELSE SUM(NVL(TE.M94,0)) + SUM(NVL(TE.M91,0)) END AS M94,
    SUM(TE.M99) AS M99,
    CASE WHEN SUM(TE.M71) IS NULL and SUM(TE.M81) IS NULL AND SUM(TE.M82) IS NULL THEN NULL
        ELSE SUM(NVL(TE.M71,0)) + SUM(NVL(TE.M81,0)) + SUM(NVL(TE.M82,0)) END AS M81_M82,
    CASE WHEN SUM(TE.M72) IS NULL AND SUM(TE.M63) IS NULL AND SUM(TE.M64) IS NULL AND SUM(TE.M65) IS NULL
                and SUM(TE.M66) IS NULL AND SUM(TE.M67) IS NULL THEN NULL
        ELSE CASE WHEN '2004' = '2002' THEN
                SUM(NVL(M63,0)) + SUM(NVL(M66,0)) + SUM(NVL(M67,0))
              ELSE
                SUM(NVL(TE.M72,0)) + SUM(NVL(TE.M63,0))
              + SUM(NVL(TE.M64,0)) + SUM(NVL(TE.M65,0))
              + SUM(NVL(TE.M66,0)) + SUM(NVL(TE.M67,0)) END  END AS M66_M67,
   CASE WHEN SUM(M10) IS NULL AND SUM(M40M) IS NULL AND SUM(M41) IS NULL AND SUM(M61M) IS NULL
        AND SUM(M62) IS NULL and SUM(P81D) IS NULL AND SUM(M66) IS NULL
        AND SUM(M67) IS NULL and SUM(M73) IS NULL AND SUM(M63) IS NULL
        AND SUM(M79) IS NULL and SUM(M90) IS NULL AND SUM(M72) IS NULL
        AND SUM(M91) IS NULL AND SUM(M94) IS NULL and SUM(M99) IS NULL THEN NULL
        ELSE CASE WHEN '2004' = '2002' THEN
                SUM(NVL(M10,0)) + SUM(NVL(M40M,0))
              + SUM(NVL(M41,0)) + SUM(NVL(M61M,0))
              + SUM(NVL(M62,0)) + SUM(NVL(P81D,0))
              + SUM(NVL(M63,0)) + SUM(NVL(M66,0))
              + SUM(NVL(M67,0)) + SUM(NVL(M73,0))
              + SUM(NVL(M79,0)) + SUM(NVL(M90,0))
              + SUM(NVL(M91,0)) + SUM(NVL(M94,0))
              + SUM(NVL(M99,0))
              WHEN '2004' >= '2003' THEN
              SUM(NVL(M10,0)) + SUM(NVL(M40M,0))
            + SUM(NVL(M41,0)) + SUM(NVL(M61M,0))
            + SUM(NVL(M62,0)) + SUM(NVL(P81D,0))
            + SUM(NVL(M82,0)) + SUM(NVL(M71,0))
            + SUM(NVL(M66,0)) + SUM(NVL(M67,0))
            + SUM(NVL(M63,0))
            + SUM(NVL(M73,0)) + SUM(NVL(M79,0))
            + SUM(NVL(M90,0))  + SUM(NVL(M91,0))
            + SUM(NVL(M94,0)) + SUM(NVL(M99,0))
              ELSE
                SUM(NVL(M10,0)) + SUM(NVL(M40M,0))
              + SUM(NVL(M41,0)) + SUM(NVL(M61M,0))
              + SUM(NVL(M62,0)) + SUM(NVL(P81D,0))
              + SUM(NVL(M82,0)) + SUM(NVL(M66,0))
              + SUM(NVL(M67,0)) + SUM(NVL(M73,0))
              + SUM(NVL(M79,0)) + SUM(NVL(M90,0))
              + SUM(NVL(M91,0)) + SUM(NVL(M94,0))
              + SUM(NVL(M99,0))
       END END AS SUBTOT_OTHER_OFFSITE,
   SUM(TE.TOTAL_OFFSITE_RELEASE) AS TOTAL_OFFSITE,
   SUM(TE.TOTAL_ONOFFSITE) AS TOTAL_ONOFFSITE,
   SUM(TE.OTOTAL_ONOFFSITE) AS TOTAL_ADJ_ONOFFSITE
      from TRI_EX_RELEASE_REPORT_INFO TE
      left outer join (select TF.TRI_FACILITY_ID, count(distinct doc_ctrl_num) NUM_R_FORMS
      from TRI_EX_RELEASE_REPORT_INFO TF
      where TF.FORM_TYPE_IND = 'L' and TF.tri_chem_id = 'N150' and  REPORTING_YEAR = '2004' and TF.ZIP_CODE = '70427' group by TF.TRI_FACILITY_ID ) R_FORM on TE.TRI_FACILITY_ID = R_FORM.TRI_FACILITY_ID
      left outer join (select TF.TRI_FACILITY_ID, count(distinct doc_ctrl_num) NUM_A_FORMS
      from TRI_EX_RELEASE_REPORT_INFO TF
      where TF.FORM_TYPE_IND = 'S' and TF.tri_chem_id = 'N150' and  REPORTING_YEAR = '2004' and TF.ZIP_CODE = '70427'  group by TF.TRI_FACILITY_ID ) A_FORM on TE.TRI_FACILITY_ID = A_FORM.TRI_FACILITY_ID
         where  REPORTING_YEAR = '2004' and TE.ZIP_CODE = '70427'   and TRI_CHEM_ID =  'N150'   group by FACILITY_NAME , FACILITY_ADDRESS,
      TE.TRI_FACILITY_ID ,
      R_FORM.NUM_R_FORMS, A_FORM.NUM_A_FORMS, TRI_CENTROID_LAT, TRI_CENTROID_LONG    order by  1  asc ) a  where ROWNUM < 101  -->

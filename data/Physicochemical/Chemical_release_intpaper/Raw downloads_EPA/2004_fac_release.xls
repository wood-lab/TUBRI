CASE WHEN  R_TE.UNINJ_I IS NULL and R_TE.RCRA_C IS NULL AND R_TE.OTH_LANDF IS NULL AND R_TE.AIR_FUG IS NULL AND R_TE.AIR_STACK IS NULL
             AND R_TE.WATER IS NULL AND R_TE.UNINJ_IIV IS NULL AND R_TE.LAND_TREA IS NULL AND R_TE.SI_5_5_3A IS NULL AND R_TE.SI_5_5_3B IS NULL
             AND R_TE.OTH_DISP IS NULL AND R_TE.M81 IS NULL and R_TE.M65 IS NULL AND R_TE.M64 IS NULL AND R_TE.M10 IS NULL AND R_TE.M41 IS NULL AND R_TE.M62 IS NULL

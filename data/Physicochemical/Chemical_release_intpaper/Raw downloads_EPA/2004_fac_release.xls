      where TF.FORM_TYPE_IND = 'S'
      and TF.TRI_CHEM_ID = 'N150' and  REPORTING_YEAR = '2004' and TF.ZIP_CODE = '70427'  group by TF.TRI_FACILITY_ID) A_FORM
      on TE.TRI_FACILITY_ID = A_FORM.TRI_FACILITY_ID
